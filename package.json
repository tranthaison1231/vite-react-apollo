    "@base-projects/web-uikit": "^1.9.8",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@sentry/integrations": "^7.11.1",
    "@sentry/react": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "@tinymce/tinymce-react": "^4.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/react-color": "^3.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "antd": "^4.22.6",
    "antd-img-crop": "^4.2.4",
    "apollo-link-error": "^1.1.13",
    "aws-amplify": "^4.3.31",
    "axios": "^0.27.2",
    "bezier-easing": "^2.1.0",
    "dayjs": "^1.11.5",
    "graphql": "^16.6.0",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "less-vars-to-js": "^1.3.0",
    "lodash-es": "^4.17.21",
    "query-string": "^7.1.1",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.4",
    "react-quill": "^2.0.0",
    "react-router-dom": "6.3.0",
    "react-virtualized": "^9.22.3",
    "recharts": "^2.1.13",
    "remark-gfm": "^3.0.1",
    "tinycolor2": "^1.4.2",
    "tinymce-react": "^1.3.2",
    "ts-node": "^10.9.1",
    "use-places-autocomplete": "^4.0.0",
    "vite-plugin-antd-dayjs": "^1.0.0",
    "vite-plugin-imp": "^2.2.0",
    "vite-plugin-sentry": "^1.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@enouvo-packages/eslint-config-react": "^1.1.3",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/import-types-preset": "^2.2.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.6",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
