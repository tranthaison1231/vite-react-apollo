{
  "name": "smartos-superadmin-web-new",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "yarn check-types && vite build",
    "start": "vite preview --port 3000",
    "check-types": "tsc --noEmit --pretty",
    "check-types:watch": "tsc --noEmit --watch",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --fix",
    "lint-staged": "lint-staged",
    "lint-pass": "echo '\\033[4;32m♡ ' Format complete ♡' \\033[0m'",
    "gen:i18:keys": "ts-node --project scripts/tsconfig.json scripts/generateTranslationTypes.ts",
    "gen-graphql-types": "graphql-codegen -r dotenv/config --config codegen.yml",
    "gen-graphql-types-country": "graphql-codegen -r dotenv/config --config codegen-country.yml",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@ant-design/pro-card": "^2.1.1",
    "@ant-design/pro-layout": "^7.3.3",
    "@apollo/client": "3.7.1",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@enouvo/react-uikit": "^0.0.13",
    "@sentry/integrations": "^7.23.0",
    "@sentry/react": "^7.23.0",
    "@sentry/tracing": "^7.23.0",
    "antd": "^4.23.6",
    "apollo-link-error": "^1.1.13",
    "aws-amplify": "^5.0.4",
    "axios": "^1.2.0",
    "dayjs": "^1.11.6",
    "graphql": "^16.6.0",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-router-dom": "6.4.4",
    "vite-plugin-pages": "^0.27.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@enouvo/eslint-config-react": "^1.5.0",
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^2.8.3",
    "@graphql-codegen/typescript-operations": "^2.5.8",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.10",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-antd-dayjs": "^1.0.0",
    "vite-plugin-imp": "^2.3.1",
    "vite-plugin-react-layouts": "^0.0.1",
    "vite-plugin-sentry": "^1.1.6",
    "vite-plugin-svgr": "^2.2.2"
  },
  "eslintIgnore": [
    "vite.config.ts",
    "scripts/*",
    "country-schemas.tsx",
    "src/vite-env.d.ts"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
